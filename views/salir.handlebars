<h2 class="page-header">Sale Vehiculo</h2>
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}
<form method="post" action="/users/salir">
    <div class="form-group">
        <label>Placas Vehiculo</label>
        <input type="text" class="form-control" placeholder="Placas Vehiculo" name="placas" pattern="[A-Z]{3}[0-9]{3}" title="Placas Formato XXX000">
    </div>
    <button type="submit" class="btn btn-default">Enviar</button>
</form>